{
    builder.UseCryptographicAlgorithms(<span class=3D"code-keyword">new</sp=
an> AuthenticatedEncryptorConfiguration
    {
        EncryptionAlgorithm =3D EncryptionAlgorithm.AES_256_CBC,
        ValidationAlgorithm =3D ValidationAlgorithm.HMACSHA256,

    }).SetDefaultKeyLifetime(TimeSpan.FromDays(365*15)).PersistKeysToFileSy=
stem
